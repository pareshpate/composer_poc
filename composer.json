{
    "name": "paresh/composer_poc",
    "description": "POC to download the custom module in custom directory",
    "type": "random-type",
    "authors": [
        {
            "name": "Paresh Patel",
            "email": "pareshpate@cybage.com"
        }
    ],
    "require": {
        "davidbarratt/custom-installer": "1.0.*@dev",
        "composer/installers": "^1.2"
    },
    "extra": {
        "custom-installer": {
            "custom/{$type}/{$vendor}/{$name}/": ["type:random-type"]
        }
    }
}
