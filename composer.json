{
    "name": "paresh/composer_poc",
    "description": "POC to download the custom module in custom directory",
    "type": "paresh",
    "authors": [
        {
            "name": "Paresh Patel",
            "email": "pareshpate@cybage.com"
        }
    ],
    "autoload": {
        "psr-0": { "Composer\\Installers\\": "src/" }
    },
    "extra": {
        "class": "Composer\\Installers\\Installer",
        "installer-disable": [
          "drupal",
          "all"
        ]
    }
}
